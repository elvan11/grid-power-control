      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              ""
            ],
            "type": "text/javascript",
            "packages": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "var resp = JSON.parse(responseBody);",
              "if (resp.data.stationStatusVo.normal === 0) {",
              "    console.log(\"no enabled station\")",
              "    return",
              "}",
              "",
              "for(var record of resp.data.page.records){",
              "    if (record.state != \"1\") {",
              "        continue;",
              "    }",
              "    console.log(\"id: \" + record.id);",
              "    console.log(\"cityStr: \" + record.cityStr);",
              "    console.log(\"sno: \" + record.sno);",
              "    pm.collectionVariables.set(\"powerStationID\", record.id);",
              "    console.log(\"enable this record\");",
              "    break;",
              "};"
            ],
            "type": "text/javascript",
            "packages": {}
          }
        }
      ],
